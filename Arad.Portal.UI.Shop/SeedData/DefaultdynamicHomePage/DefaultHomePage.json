                        "ColData": "<div target=\"editor\"><div class=\"aradInGlance fs-4\">\n<div class=\"col-10 m-auto\">\n<div class=\"row\">\n<div class=\"container\">\n<div class=\"row\">\n<div class=\"col-6 col-lg-3 delay1\" data-animation=\"animated zoomIn\">\n<div class=\"glanceItems fs-4\">2000+\n<p>مشتریان</p>\n</div>\n</div>\n\n<div class=\"col-6 col-lg-3 delay2\" data-animation=\"animated zoomIn\">\n<div class=\"glanceItems fs-4\">50,000,000+\n<p>تراکنش های روزانه</p>\n</div>\n</div>\n\n<div class=\"col-6 col-lg-3 delay3\" data-animation=\"animated zoomIn\">\n<div class=\"glanceItems fs-4\">15+\n<p>سال تجربه</p>\n</div>\n</div>\n\n<div class=\"col-6 col-lg-3 delay4\" data-animation=\"animated zoomIn\">\n<div class=\"glanceItems fs-4\">30+\n<p>کارشناس فعال</p>\n</div>\n</div>\n</div>\n</div>\n</div>\n</div>\n</div>\n<style type=\"text/css\">.glanceItems {\n        color: white;\n        background-image: url(/ckEditorDomainImages/93dfdcee-4b57-420b-bc46-19b63a573198.png);\n        background-repeat: no-repeat;\n        background-position: center center;\n        text-align: center;\n        min-width: 246px;\n        min-height: 278px;\n        padding: 100px 0;\n    }\n</style></div>",
